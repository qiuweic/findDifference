<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--    选中状态-->
    <item android:drawable="@drawable/ic_switch_on_100" android:state_checked="true"/>
    <!--    不选中状态-->
    <item android:drawable="@drawable/ic_switch_off_100" android:state_checked="false"/>
    <item android:drawable="@drawable/ic_switch_on_100" android:state_pressed="true"/>
    <!--    默认状态-->
    <item android:drawable="@drawable/ic_switch_off_100"/>
</selector>